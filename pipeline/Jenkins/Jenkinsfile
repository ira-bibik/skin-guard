pipeline {
    agent any
    stages {
        stage('Test') {
            steps {
                sh "echo secceful"
            }
        }
        stage('Build front') {
            steps {
                dir('pipeline/Docker/build/front') {
                    sh "docker built -t static:latest ."
                }
            }
        }
        stage('Build back') {
            steps {
                dir('pipeline/Docker/build/back') {
                    sh "docker build -t back:latest ."
                }
            }
        }
        stege('Deploy to stage') {
            steps {
                dir('skin-guard/pipeline/Docker/compose')
                    sh "docker compose up ."
            }
        }
        


    }

}
